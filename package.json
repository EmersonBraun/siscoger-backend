    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "7.6.1",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.0.1",

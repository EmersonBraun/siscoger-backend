    "cache-manager": "^3.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^0.0.0-typescript-beta-7",
    "faker": "^5.1.0",
    "figures": "^3.2.0",
